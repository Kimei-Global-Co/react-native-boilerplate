{
  "name": "RNBoilerplate",
  "version": "1.0.0",
  "main": "index.ts",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "scripts": {
    "start": "EXPO_USE_FAST_RESOLVER=1 expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "commit": "cz",
    "lint": "npx eslint --fix .",
    "depcheck": "npx depcheck",
    "lint:husky": "eslint $(git diff --staged --name-only | grep -E '(.js$|.ts$|.tsx$)')"
  },
  "dependencies": {
    "@discord/bottom-sheet": "bluesky-social/react-native-bottom-sheet",
    "@hookform/resolvers": "^5.0.1",
    "@react-navigation/bottom-tabs": "^7.3.13",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/native-stack": "^7.3.13",
    "@tanstack/react-query": "^5.76.1",
    "babel-plugin-react-compiler": "^19.0.0-beta-af1b7da-20250417",
    "dayjs": "^1.11.13",
    "eas-build-cache-provider": "^16.4.2",
    "expo": "~53.0.9",
    "expo-build-properties": "^0.14.6",
    "expo-image": "^2.1.7",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "^5.0.7",
    "ky": "^1.8.1",
    "lodash.get": "^4.4.2",
    "react": "19.0.0",
    "react-hook-form": "^7.56.3",
    "react-native": "0.79.2",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-keyboard-controller": "^1.17.1",
    "react-native-mmkv": "^3.2.0",
    "react-native-reanimated": "^3.17.5",
    "react-native-safe-area-context": "^5.4.0",
    "react-native-screens": "^4.10.0",
    "react-native-unistyles": "^2.32.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@types/lodash.get": "^4.4.9",
    "@types/react": "~19.0.10",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "husky": "^9.1.7",
    "typescript": "~5.8.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
