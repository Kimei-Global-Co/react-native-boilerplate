{
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "files": [
    "!src/features/__DEV__"
  ],
  "dependencies": {
    "@discord/bottom-sheet": "discord/react-native-bottom-sheet#discord-fork-5.1.6",
    "@expo-google-fonts/nunito": "^0.4.2",
    "@formatjs/intl-locale": "^4.2.11",
    "@formatjs/intl-pluralrules": "^5.4.4",
    "@hookform/resolvers": "^5.2.2",
    "@lingui/core": "^5.3.2",
    "@lingui/react": "^5.3.2",
    "@lodev09/react-native-true-sheet": "^3.8.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.3.13",
    "@react-navigation/native": "^7.1.9",
    "@react-navigation/native-stack": "^7.3.13",
    "@shopify/flash-list": "2.0.2",
    "@tanstack/react-query": "^5.90.2",
    "dayjs": "^1.11.13",
    "expo": "~54.0.33",
    "expo-build-properties": "~1.0.10",
    "expo-clipboard": "~8.0.8",
    "expo-dev-client": "~6.0.20",
    "expo-font": "~14.0.11",
    "expo-image": "~3.0.11",
    "expo-image-picker": "~17.0.10",
    "expo-localization": "~17.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "~29.0.16",
    "ky": "^1.8.2",
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "mutative": "^1.3.0",
    "react": "19.1.0",
    "react-hook-form": "^7.71.1",
    "react-native": "0.81.5",
    "react-native-drawer-layout": "^4.1.12",
    "react-native-fast-shimmer": "^1.3.2",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keyboard-controller": "1.18.5",
    "react-native-mmkv": "^3.2.0",
    "react-native-pager-view": "6.9.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-vision-camera": "^4.7.1",
    "react-native-worklets": "0.5.1",
    "zod": "^4.3.6",
    "zustand": "^5.0.11",
    "zustand-mutative": "^1.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@biomejs/biome": "2.2.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@lingui/babel-plugin-lingui-macro": "^5.3.2",
    "@lingui/cli": "^5.3.2",
    "@lingui/macro": "^5.3.2",
    "@lingui/metro-transformer": "^5.3.2",
    "@rozenite/metro": "^1.0.0-alpha.8",
    "@rozenite/mmkv-plugin": "^1.0.0-alpha.11",
    "@rozenite/network-activity-plugin": "^1.0.0-alpha.11",
    "@rozenite/tanstack-query-plugin": "^1.0.0-alpha.11",
    "@testing-library/react-native": "^13.2.2",
    "@types/jest": "29.5.14",
    "@types/lodash.debounce": "^4.0.9",
    "@types/lodash.get": "^4.4.9",
    "@types/react": "~19.1.10",
    "@types/react-test-renderer": "19.0.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-react-compiler": "1.0.0",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "danger": "^13.0.4",
    "eas-build-cache-provider": "^16.4.2",
    "expo-atlas": "0.4.3",
    "husky": "^9.1.7",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.17",
    "lint-staged": "^16.0.0",
    "patch-package": "github:evelant/patch-package#master",
    "postinstall-postinstall": "^2.1.0",
    "react-test-renderer": "19.0.0",
    "reassure": "^1.4.0",
    "svgo": "^4.0.0",
    "typescript": "~5.9.2"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "bun@1.2.13",
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,jsonc,css}": [
      "biome lint --write --staged",
      "biome format --write --staged"
    ]
  },
  "main": "index.ts",
  "name": "RNBoilerplate",
  "private": true,
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "bundle": "EXPO_UNSTABLE_METRO_OPTIMIZE_GRAPH=1 EXPO_UNSTABLE_TREE_SHAKING=1 expo export --dev=true --output-dir=dist --platform=ios --no-minify --no-bytecode",
    "bundle:visualizer": "EXPO_ATLAS=1 bunx expo start --dev-client --no-dev",
    "android:dev": "STAGE=development NODE_ENV=development expo run:android",
    "ios:dev": "STAGE=development NODE_ENV=development expo run:ios",
    "prebuild:development": "STAGE=development NODE_ENV=development npx expo prebuild",
    "build:development:ios": "STAGE=development NODE_ENV=development eas build --profile development --platform ios && say done",
    "build:development:android": "STAGE=development NODE_ENV=development eas build --profile development --platform android && say done",
    "update:development": "STAGE=development eas update --branch development",
    "android:stag": "STAGE=staging expo run:android",
    "ios:stag": "STAGE=staging expo run:ios",
    "prebuild:staging": "STAGE=staging npx expo prebuild",
    "build:staging:ios": "STAGE=staging eas build --profile preview --platform ios && say done",
    "build:staging:android": "STAGE=staging eas build --profile preview --platform android && say done",
    "update:staging": "STAGE=staging eas update --branch staging",
    "android:prod": "STAGE=production expo run:android",
    "ios:prod": "STAGE=production expo run:ios",
    "prebuild:production": "STAGE=production npx expo prebuild",
    "build:production:ios": "STAGE=production eas build --profile production --platform ios && say done",
    "build:production:android": "STAGE=production eas build --profile production --platform android && say done",
    "update:production": "STAGE=production eas update --branch production",
    "commit": "cz",
    "depcheck": "bunx depcheck",
    "env:pull": "eas env:pull",
    "intl:build": "lingui extract --overwrite --clean",
    "lint": "bunx biome check --error-on-warnings --no-errors-on-unmatched --staged ./**/*.{js,jsx,ts,tsx}",
    "icons:optimize": "svgo -f ./src/assets/icons",
    "perf-test": "reassure",
    "postinstall": "grep -s -q '^1$' ../.skip-template-cleanup || (bun scripts/clean-up.js && patch-package && bun intl:build)",
    "prebuild": "expo prebuild",
    "prepare": "husky"
  },
  "version": "1.0.0"
}
