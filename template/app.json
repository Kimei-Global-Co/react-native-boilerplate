{
  "expo": {
    "name": "RNBoilerplate",
    "slug": "boilerplate-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bitcode": false,
      "bundleIdentifier": "com.boilerplate.mobile",
      "scheme": ["com.boilerplate.mobile"],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleLocalizations": ["en", "vi"],
        "CADisableMinimumFrameDurationOnPhones": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon-android-foreground.png",
        "backgroundImage": "./src/assets/icon-android-background.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.boilerplate.mobile",
      "scheme": ["com.boilerplate.mobile"]
    },
    "experiments": {
      "buildCacheProvider": "eas",
      "reactCompiler": true
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true
          }
        }
      ],
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./src/assets/splash-icon.png",
          "imageWidth": 200
        }
      ],
      ["expo-localization"],
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/nunito/400Regular/Nunito_400Regular.ttf",
            "node_modules/@expo-google-fonts/nunito/900Black/Nunito_900Black.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "013b02d5-e722-4a67-9d71-06be7e2e239b"
      }
    },
    "owner": "kimei-global"
  }
}
